		A.OI_SEND_STATE, 
  		A.OI_SELF_UPDATE_TIME, 
  		A.OI_SUB_UPDATE_TIME, 
  		A.OI_B_UUID, 
  		A.OI_DL_UUID, 
  		A.OI_SELF_SEQUENCE, 
		A.OI_ALL_NUM1, 
  		A.OI_SELF_BATCH_SEQUENCE, 
  		A.OI_TOTAL_SEQUENCE, 
  		A.OI_ILLEGALITY_NUM, 
  		A.OI_SECOND_NUM, 
		A.OI_COMMERCE_CODE, 
  		A.OI_COMMERCE_NAME, 
  		A.OI_LOCAL_START_TIME, 
  		A.OI_STANDARD_START_TIME, 
		A.OI_MONITOR_STATUS, 
  		A.OI_TIME_MONITOR_STATUS, 
  		A.OI_STANDARD_TIME_TYPE, 
  		A.OI_IN_FEED_STATE,
	    B.OCI_ID, 
		B.OCI_UUID, 
  		B.OCI_CIG_BRAND, 
  		B.OCI_CIG_TRADEMARK, 
  		B.OCI_IS_ABNORMAL, 
  		B.OCI_IS_PRINT, 
		B.OCI_ORDER_NUM, 
  		B.OCI_ORDER_NUM1, 
  		B.OCI_ACTUAL_PRICE, 
  		B.OCI_STANDERD_PRICE, 
  		B.OCI_LOCK_STEP, 
		B.OCI_LOCK_STATUS, 
  		B.OCI_CUSTOM_INFO,
	  	C.OB_ID, 
	  	C.OB_UUID, 
	  	C.OB_SEQUENCE, 
	  	C.OB_SORT_DATE, 
	  	C.OB_CIG_DERIVE_CODE, 
	  	C.OB_CUSTOM_INFO, 
	  	C.OB_RETAILER_CODE, 
	    C.OB_BCIG_BARCODE, 
	  	C.OB_TOTAL_SEQUENCE, 
	  	C.OB_LOCK_STEP, 
	  	C.OB_LOCK_STATUS,
	  	D.B_ID, 
		D.B_UUID, 
		D.B_SORT_DATE, 
		D.B_CODE, 
		D.B_SEQUENCE, 
		D.B_TYPE, 
		D.B_IS_CHANGE_SLINE, 
		D.B_SLINE_CODE,
		D.B_ORDER_NUM, 
		D.B_SHOULD_SEND_NUM, 
		D.B_THIS_SEND_NUM, 
		D.B_ALL_RECV_NUM, 
		D.B_PRINT_NUM, 
		D.B_START_TIME,
		D.B_END_TIME, 
		D.B_STATE, 
		D.B_SEND_STATE, 
		D.B_SELF_UPDATE_TIME, 
		D.B_SUB_UPDATE_TIME,
	  	E.DL_ID, 
  		E.DL_UUID, 
  		E.DL_SORT_DATE, 
  		E.DL_B_UUID, 
  		E.DL_B_CODE, 
  		E.DL_B_TYPE, 
  		E.DL_CODE, 
  		E.DL_NAME, 
		E.DL_SEQUENCE, 
  		E.DL_ORDER_NUM, 
