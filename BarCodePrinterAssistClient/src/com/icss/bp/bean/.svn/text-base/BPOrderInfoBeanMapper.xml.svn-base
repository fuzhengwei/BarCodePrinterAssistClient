  		A.OI_TOTAL_SEQUENCE, 
  		A.OI_ILLEGALITY_NUM, 
  		A.OI_SECOND_NUM, 
		A.OI_COMMERCE_CODE, 
  		A.OI_COMMERCE_NAME, 
  		A.OI_LOCAL_START_TIME, 
  		A.OI_STANDARD_START_TIME, 
		A.OI_MONITOR_STATUS, 
  		A.OI_TIME_MONITOR_STATUS, 
  		A.OI_STANDARD_TIME_TYPE, 
  		A.OI_IN_FEED_STATE,
	    B.OCI_ID, 
		B.OCI_UUID, 
  		B.OCI_CIG_BRAND, 
  		B.OCI_CIG_TRADEMARK, 
  		B.OCI_IS_ABNORMAL, 
