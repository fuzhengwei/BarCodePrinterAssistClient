		D.B_SORT_DATE, 
		D.B_CODE, 
		D.B_SEQUENCE, 
		D.B_TYPE, 
		D.B_IS_CHANGE_SLINE, 
		D.B_SLINE_CODE,
		D.B_ORDER_NUM, 
		D.B_SHOULD_SEND_NUM, 
		D.B_THIS_SEND_NUM, 
		D.B_ALL_RECV_NUM, 
		D.B_PRINT_NUM, 
		D.B_START_TIME,
		D.B_END_TIME, 
		D.B_STATE, 
		D.B_SEND_STATE, 
		D.B_SELF_UPDATE_TIME, 
		D.B_SUB_UPDATE_TIME,
	  	E.DL_ID, 
  		E.DL_UUID, 
  		E.DL_SORT_DATE, 
  		E.DL_B_UUID, 
  		E.DL_B_CODE, 
  		E.DL_B_TYPE, 
  		E.DL_CODE, 
  		E.DL_NAME, 
		E.DL_SEQUENCE, 
  		E.DL_ORDER_NUM, 
  		E.DL_PRINT_NUM, 
  		E.DL_START_TIME, 
  		E.DL_END_TIME, 
  		E.DL_STATE, 
  		E.DL_SEND_STATE, 
		E.DL_SELF_UPDATE_TIME, 
  		E.DL_SUB_UPDATE_TIME
	FROM BP_ORDER_INFO A 
	     JOIN BP_ORDER_CIG_INFO B ON A.OI_UUID = B.OCI_UUID 
	     JOIN BP_ORDER_BARCODE C ON A.OI_UUID = C.OB_UUID
	     JOIN BP_BATCH D ON A.OI_B_UUID = D.B_UUID
	     JOIN BP_DIST_LINE E ON A.OI_DL_UUID = E.DL_UUID
  	WHERE A.OI_BASE_FEED_STATE = 0 
  	      OR A.OI_BASE_FEED_STATE IS NULL
  	      AND A.OI_STATE = 3
  	
  	UNION ALL 
  	
  	SELECT  TOP 6
		A.OI_ID, 
  	    A.OI_UUID, 
  	    A.OI_B_CODE, 
  		A.OI_B_TYPE, 
  		A.OI_ISCHANGE_SLINE, 
  		A.OI_DL_CODE, 
  		A.OI_DL_NAME, 
		A.OI_SLINE_CODE, 
  		A.OI_SLINE_NAME, 
  		A.OI_SEQUENCE, 
  		A.OI_CODE, 
  		A.OI_RETAILER_CODE, 
  		A.OI_RETAILER_NAME, 
		A.OI_DATE, 
  		A.OI_SORT_DATE, 
  		A.OI_CIG_FLAG, 
  		A.OI_ALL_NUM, 
  		A.OI_NORMAL_NUM, 
  		A.OI_CUSTOM_INFO, 
  		A.OI_CHECK_NUM, 
		A.OI_SEND_NUM, 
  		A.OI_PRINT_NUM, 
