  		A.OI_SEQUENCE, 
  		A.OI_CODE, 
  		A.OI_RETAILER_CODE, 
  		A.OI_RETAILER_NAME, 
		A.OI_DATE, 
  		A.OI_SORT_DATE, 
  		A.OI_CIG_FLAG, 
  		A.OI_ALL_NUM, 
  		A.OI_NORMAL_NUM, 
  		A.OI_CUSTOM_INFO, 
  		A.OI_CHECK_NUM, 
		A.OI_SEND_NUM, 
  		A.OI_PRINT_NUM, 
